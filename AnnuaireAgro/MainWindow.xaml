<Window x:Class="AnnuaireAgro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       
        Title="AnnuaireAgro" Height="600" Width="800"  Background="#FF060621" MinHeight="400" MinWidth="600">


    <Grid Margin="0,0,0,0">


        <TextBox HorizontalAlignment="Left" Height="35" Margin="10,10,0,0" VerticalAlignment="Top" Width="99" KeyDown="TextBox_KeyDown" Background="{x:Null}" BorderBrush="{x:Null}" Grid.Row="1"/>

        <Image VerticalAlignment="Top" HorizontalAlignment="Center" x:Name="logo" Source="C:\DEV\AnnuaireAgro\img\logo.png" Height="202" Width="172" Margin="0,52,0,0" />



        <TextBlock x:Name="txtbAccueil" Text="Bienvenu(e), veuillez cliquer sur le bouton ci-dessous pour accèder à l'annuaire du personnel" TextAlignment="Center" TextWrapping="Wrap" Foreground="#FF50C1C1" VerticalAlignment="Center" FontSize="24" HorizontalAlignment="Center" Width="566"/>
        <Frame x:Name="Main" Margin="0,20,0,0" NavigationUIVisibility="Hidden" Height="auto" VerticalAlignment="Top" Grid.Row="1" />

        <Button x:Name="btnCollab" Margin="0 0 0 40" Content="Collaborateurs" Background="#FF246887" Foreground="#FFD2F1F0" FontSize="14" BorderBrush="#FF439C98" Click="btnCollab_Click" HorizontalAlignment="Center" VerticalAlignment="Bottom" />



    </Grid>
</Window>


